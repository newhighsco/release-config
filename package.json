{
  "name": "@newhighsco/release-config",
  "description": "New High Score shareable config for semantic-release",
  "version": "1.0.15",
  "author": "New High Score <hello@newhighsco.re>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/release-config.git"
  },
  "homepage": "https://github.com/newhighsco/release-config#readme",
  "bugs": {
    "url": "https://github.com/newhighsco/release-config/issues"
  },
  "main": "release.config.js",
  "scripts": {
    "test": "npm run lint",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint --cache --ext .js,.jsx,.json .",
    "format": "npm-run-all --parallel format:*",
    "format:js": "npm run lint:js -- --fix"
  },
  "dependencies": {
    "@semantic-release/changelog": "5.0.0",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.0.4",
    "@semantic-release/npm": "7.0.4",
    "@semantic-release/release-notes-generator": "9.0.1"
  },
  "devDependencies": {
    "@newhighsco/eslint-config": "1.1.1",
    "@newhighsco/prettier-config": "1.0.2",
    "@newhighsco/renovate-config": "1.1.0",
    "eslint": "6.8.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1"
  },
  "prettier": "@newhighsco/prettier-config",
  "eslintConfig": {
    "extends": [
      "@newhighsco"
    ]
  },
  "release": {
    "extends": "./release.config.js"
  },
  "renovate": {
    "extends": [
      "@newhighsco"
    ]
  }
}
